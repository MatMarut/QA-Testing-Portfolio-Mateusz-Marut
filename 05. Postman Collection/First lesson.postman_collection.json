    {
      "name": "Post petstore",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code is 200\", function () {\r",
              "    pm.response.to.have.status(200);\r",
              "});\r",
              "pm.test(\"Response time under 200ms\", function() {\r",
              "    pm.expect(pm.response.responseTime).to.be.below(200)\r",
              "});\r",
              "pm.test(\"Response has valid scheme\", function() {\r",
              "    const response = pm.response.json();\r",
              "\r",
              "    pm.expect(response).to.be.an('object');\r",
              "    pm.expect(response).to.have.all.keys('id', 'category', 'name', 'photoUrls', 'tags', 'status');\r",
              "\r",
              "    pm.expect(response.category).to.be.an('object');\r",
              "    pm.expect(response.category).to.have.all.keys('id', 'name');\r",
              "    pm.expect(response.tags).to.be.an('array');\r",
              "});\r",
              "pm.test(\"Status has the correct value\", function() {\r",
              "\r",
              "    pm.expect(pm.response.json()).to.be.an('object');\r",
              "    pm.expect(pm.response.json().status).to.be.a('string');\r",
              "    pm.expect(pm.response.json().status).to.be.oneOf(['available', 'unavailable', 'pending', 'sold']);\r",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
